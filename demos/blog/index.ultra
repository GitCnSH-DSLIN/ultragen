@{
    live: 'Hello, world'
    ###
    # include: dirSep('templates/login.ultra.html')
    createGen: 'routes', 'config/routes.gen'
    route = ~(_method, ':', _route)
    route = routeMatch(@route, 'routes', 'errors.e404')
    controller = substring(@route, 0, indexOf(@route, '.'))
    action = substring(@route, ++(indexOf(@route, '.')))
    include: dirSep(~('controllers/', @controller, '.ultra'))
    callProc: +('.', @controller, @action)
    ###
}