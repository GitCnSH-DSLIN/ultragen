function func1()
    print('resposta para chave opt1')
end

function func2()
    print('resposta para chave opt2')
end

function func3()
    print('resposta para chave opt3 e opt4')
end

function notFound()
    print('resposta para chave nao existente')
end

options = {
    'opt1': func1,
    'opt2': func2,
    ['opt3', 'opt4']: func3,
    null: notFound
}

for (options.keys(), k)
    response = options[k]
    inline(k, ': ')
    response()
end

a_null = options['non-existent-key']
a_null()