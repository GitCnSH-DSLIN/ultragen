function bg(color)
    return 'background-color: ' + color + ';'
end

function color(c)
    return 'color: ' + c + ';'
end

function padding(quant)
    return 'padding: ' + str(quant) + 'px; '
end

function margin(quant)
    return 'margin: ' + str(quant) + 'px; '
end

function rule(sel, props)
    out = sel + ' {'
    for (props, p)
        out = out + p
    end
    return out + '}'
end