function asAttrs() : Dict
    keys = self.keys()
    l = []
    for (keys, k)
        l.append(k + '="'+ (self[k]) +'"')
    end
    return ' '.join(l)
end

function toJson() : Dict
    include @Core.JsonHelpers
    input_type = typeof(self)
    if (input_type == (Dict.internal))
        out = dumpObj(self)
    else
        return 'Invalid type for conversion'
    end
    return out
end