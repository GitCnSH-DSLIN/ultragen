function reverse() : TStringInstance
    len = self.length
    res = ''
    for (len, i)
        x = len - i - 1
        res = res + (self[x])
    end
    return res
end

function isInt() : TStringInstance
    nums = '0123456789'
    ret = true
    for (self, ch)
        x = nums.pos(ch)
        
        if (x == -1)
            ret = false
            break
        end
    end
    return ret
end

function writeText(filename) : TStringInstance
    self.split('\n').writeText(filename)
end
